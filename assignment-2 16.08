#include <stdio.h>
int main() 
{
    int arr[]={12,23,17,19,29,57,30,2,7};
    int no,f;
    printf("Elements in array are:\n");
    for(int i=0;i<12;i++)
    {
        printf("%d\t",arr[i]);
    }
    for(int i=0;i<12;i++)
    {
        no=arr[i];
        f=0;
        for(int j=2;j<no;j++)
        {
            if(no%j==0)
            {
            f=1;
            break;
            }
        }
        
    if(f==0)
    {
        printf("\n%d is prime number",no);
    }
    }
    return 0;
}
